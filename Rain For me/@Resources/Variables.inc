[Variables]
; 全局字体设置
fontName=Montserrat-Light
fontNameAlt=Minecraft AE

; 全局颜色设置 - 可以在这里修改所有皮肤的颜色
primaryColor=100,180,255
secondaryColor=80,140,200
textColor=100,180,255
lightTextColor=80,80,80
backgroundColor=255,255,255,0

; 边框颜色
borderColor=0,0,0
barColor=255,255,255

; 预设颜色方案（取消注释下面的行来使用不同配色）
; 蓝色主题（当前）
primaryColor=100,180,255
secondaryColor=80,140,200

; 绿色主题
; primaryColor=100,255,180
; secondaryColor=80,200,140

; 紫色主题
; primaryColor=180,100,255
; secondaryColor=140,80,200

; 橙色主题
; primaryColor=255,180,100
; secondaryColor=200,140,80

; 红色主题
; primaryColor=255,100,100
; secondaryColor=200,80,80
